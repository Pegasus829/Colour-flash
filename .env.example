# AWS Configuration for Centralized Leaderboard with OAuth
#
# Deploy the CloudFormation template (infrastructure/cloudformation.yaml)
# then copy the values from the Outputs tab

VITE_AWS_REGION=us-east-1
VITE_AWS_USER_POOL_ID=us-east-1_xxxxxxxxx
VITE_AWS_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_AWS_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
VITE_AWS_DYNAMODB_TABLE=color-match-rush-scores
VITE_COGNITO_DOMAIN=colormatchrush-123456789012.auth.us-east-1.amazoncognito.com
VITE_REDIRECT_URI=http://localhost:5173
